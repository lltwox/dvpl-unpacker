#!/usr/bin/env node

var args = process.argv;
if (args.length != 3) {
  console.log('Usage: dvpl-unpacker <input file>');
  process.exit(1);
}

var input = args[2];
var DvplUnpacker = require('../index.js');

(async function() {
  try {
    let output = await DvplUnpacker.unpackFile(input);
    process.stdout.write(output);
  } catch (err) {
    console.log(err);
  }
}());